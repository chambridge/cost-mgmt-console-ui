[
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": "/cost-mgmt",
      "component": { "$codeRef": "ExamplePage" }
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "example",
      "name": "Cost Management",
      "href": "/cost-mgmt",
      "perspective": "admin",
      "section": "home"
    }
  },
  {
    "extensions": [
      {
        "type": "console.api.proxy",
        "properties": {
          "servicePath": "api/proxy/plugin/cost-mgmt-ui-console-plugin/backend",
          "targetEndpoint": "https://cost-proxy-costmanagement-metrics-operator.apps.costlab.openshift.mbulab.xyz"
        }
      }
    ]
  }
]
